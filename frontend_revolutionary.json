{
  "type": "app",
  "title": "üéµ Harmonic Flow",
  "description": "Immersive audio analysis through liquid glass technology",
  "theme": {
    "primary_color": "#8B5CF6",
    "secondary_color": "#EC4899", 
    "accent_color": "#F59E0B",
    "background_color": "linear-gradient(135deg, #667eea 0%, #764ba2 100%)",
    "glass_color": "rgba(255, 255, 255, 0.1)",
    "glow_color": "rgba(139, 92, 246, 0.3)"
