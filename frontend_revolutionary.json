    },
    "holographic_glow": {
      "background": "linear-gradient(45deg, rgba(139, 92, 246, 0.2), rgba(236, 72, 153, 0.2), rgba(245, 158, 11, 0.2))",
      "animation": "holographicPulse 3s ease-in-out infinite",
      "border_radius": "15px",
      "backdrop_filter": "blur(15px)"
    },
    "floating_text": {
      "font_family": "'Orbitron', 'Arial', sans-serif",
      "text_shadow": "0 0 20px rgba(139, 92, 246, 0.5)",
      "animation": "floatingText 2s ease-in-out infinite"
    },
    "neural_network_bg": {
      "background": "radial-gradient(circle at 50% 50%, rgba(139, 92, 246, 0.1) 0%, transparent 70%)",
      "animation": "neuralPulse 4s ease-in-out infinite"
    },
    "voice_wave": {
      "background": "linear-gradient(90deg, transparent, rgba(139, 92, 246, 0.3), transparent)",
      "animation": "voiceWave 1.5s ease-in-out infinite",
      "height": "3px",
      "border_radius": "2px"
    },
    "ai_suggestion": {
      "background": "linear-gradient(135deg, rgba(34, 197, 94, 0.1) 0%, rgba(59, 130, 246, 0.1) 100%)",
