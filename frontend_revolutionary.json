  },
  "animations": {
    "holographicPulse": "@keyframes holographicPulse { 0%, 100% { opacity: 0.7; transform: scale(1); } 50% { opacity: 1; transform: scale(1.02); } }",
    "floatingText": "@keyframes floatingText { 0%, 100% { transform: translateY(0px); } 50% { transform: translateY(-5px); } }",
    "neuralPulse": "@keyframes neuralPulse { 0% { transform: scale(1); opacity: 0.3; } 50% { transform: scale(1.1); opacity: 0.6; } 100% { transform: scale(1); opacity: 0.3; } }",
    "liquidFlow": "@keyframes liquidFlow { 0% { background-position: 0% 50%; } 50% { background-position: 100% 50%; } 100% { background-position: 0% 50%; } }",
    "voiceWave": "@keyframes voiceWave { 0%, 100% { transform: scaleX(0.3); opacity: 0.5; } 50% { transform: scaleX(1); opacity: 1; } }",
    "aiGlow": "@keyframes aiGlow { 0%, 100% { box-shadow: 0 0 10px rgba(34, 197, 94, 0.3); } 50% { box-shadow: 0 0 20px rgba(34, 197, 94, 0.6); } }",
    "morphingShape": "@keyframes morphingShape { 0% { border-radius: 50% 50% 50% 50% / 60% 60% 40% 40%; } 25% { border-radius: 60% 40% 30% 70% / 60% 30% 70% 40%; } 50% { border-radius: 30% 60% 70% 40% / 50% 60% 30% 60%; } 75% { border-radius: 60% 40% 30% 70% / 60% 30% 70% 40%; } 100% { border-radius: 50% 50% 50% 50% / 60% 60% 40% 40%; } }"
